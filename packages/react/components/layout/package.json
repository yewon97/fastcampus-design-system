    "clean": "rm -rf dist",
    "dev": "yarn build:js --watch && yarn build:type --watch"
  },
  "devDependencies": {
    "@fastcampus/esbuild-config": "workspace:^",
    "@fastcampus/themes": "workspace:^",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vanilla-extract/css": "^1.17.4",
    "@vanilla-extract/esbuild-plugin": "^2.3.18",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "@fastcampus/themes": "workspace:^",
    "react": "*"
  }
}
